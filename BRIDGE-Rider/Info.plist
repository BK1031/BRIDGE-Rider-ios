	<string>&quot;Please allow BRIDGE to access your location so we can route drivers to you and notify you when your driver is nearby&quot;</string>
	<key>UIAppFonts</key>
	<array>
		<string>Product Sans Regular</string>
		<string>Product Sans Bold</string>
		<string>Product Sans Italic</string>
		<string>Product Sans Bold Italic</string>
	</array>
	<key>UIApplicationExitsOnSuspend</key>
	<false/>
	<key>UIBackgroundModes</key>
	<array>
		<string>location</string>
	</array>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>gps</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>
</dict>
</plist>
